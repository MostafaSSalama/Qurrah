{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "QurrahConnectionString": "Data Source=.;Initial Catalog=Qurrah;Integrated Security=True;TrustServerCertificate=True;"
  },
  "ServiceURLs": {

    "FAQTypeAPI": "https://localhost:44379/api/FAQTypeAPI",
    "FAQAPI": "https://localhost:44379/api/FAQAPI",
    "UserAuthAPI": "https://localhost:44379/api/UserAuth",
    "LocalizationAPI": "https://localhost:44379/api/LocalizationAPI",
    "LookupAPI": "https://localhost:44379/api/LookupAPI",
    "CenterAPI": "https://localhost:44379/api/CenterAPI",
    "FileAPI": "https://localhost:44379/api/FileAPI"

  },
  "AppSettings": {
    "AssemblyName": "Qurrah.Web",
    "AllowedFileExtensions": ".pdf,.docx,.doc,.png,.jpg,.jpeg",
    "AllowedFileSize": "1048576", //1MB,
    "AllowedFilesCount": "3"
  },
  "Authentication": {
    "JWTLifetimeInMinutes": "20",
    "JWTLifetimeInDays": "365"
  },
  "AllowedHosts": "*"
}